Function STDEVP(a)
    Dim l, length, Count, SCount
    length = a.Count
    Count = 0
    SCount = 0

    For Each l In a
        Count = Count + CCur(l)
    Next

    For Each l In a
        SCount = SCount + CCur((l - CCur(Count/length))) ^ 2
    Next

    STDEVP = CCur(Sqr(Scount/length))
End Function


Dim ll
Set ll = NetBox.JsonParse("[9,12,15,18,21,24,27,30,33,36,39,42,45,48,51,54,57,60,63,66,69,72,75,78]")

Shell.Console.WriteLine STDEVP(ll)
